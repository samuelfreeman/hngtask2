{
    "version": 2,
    "name": "hngtask2",
    "builds": [
        {
            "src": "./dist/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/index.js"
        }
    ],
    "build": {
        "command": "npm run build",
        "outputDirectory": "dist"
    },
    "installCommand": "npm install",
    "devCommand": "npm run dev"
}